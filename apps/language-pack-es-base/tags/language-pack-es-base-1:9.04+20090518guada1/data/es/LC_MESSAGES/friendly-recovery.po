#: ../usr/share/recovery-mode/recovery-menu:7
#, sh-format
msgid "Recovery Menu"
msgstr "Menu de recuperación"

#: ../usr/share/recovery-mode/recovery-menu:13
#, sh-format
msgid "   Resume normal boot"
msgstr "   Continuar con el arranque normal"

#: ../usr/share/recovery-mode/options/dpkg:6
#, sh-format
msgid "Repair broken packages"
msgstr "Reparar paquetes rotos"

#: ../usr/share/recovery-mode/options/dpkg:33
#: ../usr/share/recovery-mode/options/fsck:18
#, sh-format
msgid "Finished, please press ENTER"
msgstr "Finalizado, por favor presione INTRO"

#: ../usr/share/recovery-mode/options/fsck:8
#, sh-format
msgid "File system check"
msgstr "Revisión del sistema de archivos"

#: ../usr/share/recovery-mode/options/root:6
#, sh-format
msgid "Drop to root shell prompt"
msgstr "Pasar a un intérprete de comandos como administrador"

#: ../usr/share/recovery-mode/options/xfix:11
#, sh-format
msgid "Try to auto repair graphic problems"
msgstr "Intentar reparar automáticamente los problemas gráficos"

#: ../usr/share/recovery-mode/options/xfix:15
#, sh-format
msgid ""
"The graphics reconfiguration (xserver) is now running.  Your screen may "
"flicker during hardware autodetection."
msgstr ""
"La reconfiguración de gráficos (xserver) está ahora en ejecución. Su "
"pantalla podría parpadear durante la autodetección de hardware."
