#. #define HELP
#: LYMessages.c:51
msgid ""
"Commands: Use arrow keys to move, '?' for help, 'q' to quit, '<-' to go back."
msgstr ""
"Intrucciones: Utilice las flechas para moverse, '?' para obtener ayuda, 'q' "
"para salir, '<-' para regresar."

#. #define MOREHELP
#: LYMessages.c:53
msgid ""
"-- press space for more, use arrow keys to move, '?' for help, 'q' to quit."
msgstr ""
"-- presione espacio para ver más, use las flechas para moverse, '?' para "
"ayuda, 'q' para salir."

#: LYMessages.c:54
msgid "-- press space for next page --"
msgstr "-- presione espacio para pasar a la siguiente página --"

#: LYMessages.c:55
msgid "URL too long"
msgstr "URL demasiado larga"

#. Inactive input fields, messages used with -tna option - kw
#. #define FORM_LINK_TEXT_MESSAGE_INA
#: LYMessages.c:61
msgid "(Text entry field) Inactive.  Press <return> to activate."
msgstr "(Campo de entrada de texto) Inactivo. Presione <enter> para activar."

#. #define FORM_LINK_TEXTAREA_MESSAGE_INA
#: LYMessages.c:63
msgid "(Textarea) Inactive.  Press <return> to activate."
msgstr "(Área de tecto) Inactiva. Presione <enter> para activar."

#. #define FORM_LINK_TEXTAREA_MESSAGE_INA_E
#: LYMessages.c:65
#, c-format
msgid "(Textarea) Inactive.  Press <return> to activate (%s for editor)."
msgstr ""
"(Área de tecto) Inactiva. Presione <enter> para activar (%s para el editor)."

#. #define FORM_LINK_TEXT_SUBMIT_MESSAGE_INA
#: LYMessages.c:67
msgid "(Form field) Inactive.  Use <return> to edit."
msgstr "(Formulario) Inactivo. Utilice <enter> para editar."

#. #define FORM_TEXT_SUBMIT_MESSAGE_INA_X
#: LYMessages.c:69
#, c-format
msgid ""
"(Form field) Inactive.  Use <return> to edit (%s to submit with no cache)."
msgstr ""
"(Formulario) Inactivo. Utilice <enter> para editar (%s para enviar sin "
"caché)."

#. #define FORM_TEXT_RESUBMIT_MESSAGE_INA
#: LYMessages.c:71
msgid ""
"(Form field) Inactive. Press <return> to edit, press <return> twice to "
"submit."
msgstr ""
"(Formulario) Inactivo. Presione <enter> para editar, presione dos veces "
"<enter> para enviar."

#. #define FORM_TEXT_SUBMIT_MAILTO_MSG_INA
#: LYMessages.c:73
msgid "(mailto form field) Inactive.  Press <return> to change."
msgstr "(Campo mailto) Inactivo. Presione <enter> para modificarlo."

#. #define FORM_LINK_PASSWORD_MESSAGE_INA
#: LYMessages.c:75
msgid "(Password entry field) Inactive.  Press <return> to activate."
msgstr "(Campo de contraseña) Inactivo. Presione <enter> para activar."

#. #define FORM_LINK_FILE_UNM_MSG
#: LYMessages.c:78
msgid ""
"UNMODIFIABLE file entry field.  Use UP or DOWN arrows or tab to move off."
msgstr ""
"Formulario de entrada NO EDITABLE. Utilice las teclas ARRIBA, ABAJO o el "
"tabulador para moverse fuera."

#. #define FORM_LINK_FILE_MESSAGE
#: LYMessages.c:80
msgid ""
"(File entry field) Enter filename.  Use UP or DOWN arrows or tab to move off."
msgstr ""
"(Campo de entrada de archivos) Ingrese el nombre del archivo. Presione las "
"teclas ARRIBA, ABAJO o el tabulador para moverse fuera."

#. #define FORM_LINK_TEXT_MESSAGE
#: LYMessages.c:82
msgid ""
"(Text entry field) Enter text.  Use UP or DOWN arrows or tab to move off."
msgstr ""
"(Campo de entrada de texto) Ingrese el texto. Utilice las teclas flecha "
"ARRIBA, ABAJO o el tabulador para moverse fuera."

#. #define FORM_LINK_TEXTAREA_MESSAGE
#: LYMessages.c:84
msgid "(Textarea) Enter text. Use UP/DOWN arrows or TAB to move off."
msgstr ""
"(Area de texto) Ingrese el texto. Utilice las teclas flecha ARRIBA, ABAJO o "
"el tabulador para moverse fuera."

#. #define FORM_LINK_TEXTAREA_MESSAGE_E
#: LYMessages.c:86
#, c-format
msgid ""
"(Textarea) Enter text. Use UP/DOWN arrows or TAB to move off (%s for editor)."
msgstr ""
"(Area de texto) Ingrese el texto. Utilice las teclas flecha ARRIBA, ABAJO o "
"el tabulador para moverse fuera (%s para el editor)."

#. #define FORM_LINK_TEXT_UNM_MSG
#: LYMessages.c:88
msgid ""
"UNMODIFIABLE form text field.  Use UP or DOWN arrows or tab to move off."
msgstr ""
"Campo de formulario de texto NO EDITABLE. Use las teclas ARRIBA, ABAJO o el "
"tabulador para moverse fuera."

#. #define FORM_LINK_TEXT_SUBMIT_MESSAGE
#: LYMessages.c:90
msgid "(Form field) Enter text.  Use <return> to submit."
msgstr "(Formulario) Ingrese el texto. Pulse <enter> para enviar."

#. #define FORM_LINK_TEXT_SUBMIT_MESSAGE_X
#: LYMessages.c:92
#, c-format
msgid "(Form field) Enter text.  Use <return> to submit (%s for no cache)."
msgstr ""
"(Campo de formulario) Ingrese el texto. Utilice <enter> para enviar (%s para "
"no usar el caché)."

#. #define FORM_LINK_TEXT_RESUBMIT_MESSAGE
#: LYMessages.c:94
msgid ""
"(Form field) Enter text.  Use <return> to submit, arrows or tab to move off."
msgstr ""
"(Campo de formulario) Ingrese el texto. Utilice <intro> para enviar o las "
"teclas de las flechas o el tabulador para moverse fuera."

#. #define FORM_LINK_TEXT_SUBMIT_UNM_MSG
#: LYMessages.c:96
msgid "UNMODIFIABLE form field.  Use UP or DOWN arrows or tab to move off."
msgstr ""
"Campo de texto NO EDITABLE. Presione ARRIBA, ABAJO o la tecla de tabulador "
"para moverse fuera."

#. #define FORM_LINK_TEXT_SUBMIT_MAILTO_MSG
#: LYMessages.c:98
msgid ""
"(mailto form field) Enter text.  Use <return> to submit, arrows to move off."
msgstr ""
"(Campo de formulario mailto) ingrese el texto.  Utilice <enter> para enviar "
"o las teclas de las flechas para moverse fuera."

#. #define FORM_LINK_TEXT_SUBMIT_MAILTO_DIS_MSG
#: LYMessages.c:100
msgid "(mailto form field) Mail is disallowed so you cannot submit."
msgstr ""
"(Campo de formulario mailto) el envío de correo no está permitido, por lo "
"tanto usted no puede enviarlo."

#. #define FORM_LINK_PASSWORD_MESSAGE
#: LYMessages.c:102
msgid ""
"(Password entry field) Enter text.  Use UP or DOWN arrows or tab to move off."
