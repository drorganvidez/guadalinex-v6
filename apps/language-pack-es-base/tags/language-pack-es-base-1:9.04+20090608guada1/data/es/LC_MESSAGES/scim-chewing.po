#: ../src/scim_chewing_imengine.cpp:204
msgid "Hot Keys:"
msgstr "Atajos de teclado:"

#: ../src/scim_chewing_imengine.cpp:206
msgid "    Switch between English/Chinese mode."
msgstr "    Cambiar entre modo inglés y chino."

#: ../src/scim_chewing_imengine.cpp:207
msgid ""
"\n"
"\n"
"  Space:\n"
"    Use space key to select candidate phrases.\n"
"\n"
"  Tab:\n"
"    Use tab key to dispart or connect a phrase.\n"
"\n"
"  Ctrl + [number]:\n"
"    Use ctrl + number key to add a user-defined phrase. (Here number stands "
"for the length of the user-defined phrase.)\n"
"\n"
"  Ctrl + 0:\n"
"    Use Ctrl + 0 to specify symbolic input."
msgstr ""
"\n"
"\n"
"  Barra espaciadora:\n"
"    Use la barra espaciadora para elegir entre las expresiones candidatas.\n"
"\n"
"  Tabulador:\n"
"    Use el tabulador para separar o conectar una expresión.\n"
"\n"
"  Ctrl + [número]:\n"
"    Use Ctrl + número para añadir una expresión definida por el usuario. (El "
"número indica la longitud de la expresión definida por el usuario.)\n"
"\n"
"  Ctrl + 0:\n"
"    Use Ctrl + 0 para especificar entrada simbólica."

