#: info/infodoc.c:45 info/infodoc.c:97
msgid ""
"Basic Commands in Info Windows\n"
"******************************\n"
msgstr ""
"Comandos básicos en Info Windows\n"
"******************************\n"

#: info/infodoc.c:48
msgid "\\%-10[quit-help]  Quit this help.\n"
msgstr "\\%-10[quit-help] Sale de esta ayuda.\n"

#: info/infodoc.c:49
msgid "\\%-10[quit]  Quit Info altogether.\n"
msgstr "\\%-10[quit] Sale de Info.\n"

#: info/infodoc.c:50
msgid "\\%-10[get-info-help-node]  Invoke the Info tutorial.\n"
msgstr "\\%-10[get-info-help-node] Invoca el tutorial de Info.\n"

#: info/infodoc.c:52 info/infodoc.c:104
msgid ""
"Selecting other nodes:\n"
"----------------------\n"
msgstr ""
"Seleccionando otros nodos:\n"
"----------------------\n"

#: info/infodoc.c:54
msgid "\\%-10[next-node]  Move to the \"next\" node of this node.\n"
msgstr "\\%-10[next-node] Mueve al siguiente nodo de este nodo.\n"

#: info/infodoc.c:55
msgid "\\%-10[prev-node]  Move to the \"previous\" node of this node.\n"
msgstr "\\%-10[prev-node] Mueve al anterior nodo de este nodo.\n"

#: info/infodoc.c:56
msgid "\\%-10[up-node]  Move \"up\" from this node.\n"
msgstr "\\%-10[up-node] Mueve arriba de este nodo.\n"

#: info/infodoc.c:57
msgid ""
"\\%-10[menu-item]  Pick menu item specified by name.\n"
"              Picking a menu item causes another node to be selected.\n"
msgstr ""
"\\%-10[menu-item] Selecciona un elemento de menú por el nombre.\n"
"              Seleccionar un elemento de menú hace que otro nodo sea "
"seleccionado.\n"

#: info/infodoc.c:59
msgid ""
"\\%-10[xref-item]  Follow a cross reference.  Reads name of reference.\n"
msgstr ""
"\\%-10[xref-item] Sigue una referencia cruzada. Lee el nombre de la "
"referencia.\n"

#: info/infodoc.c:60
msgid "\\%-10[history-node]  Move to the last node seen in this window.\n"
msgstr "\\%-10[history-node] Va al último nodo de esta ventana.\n"

#: info/infodoc.c:61
msgid "\\%-10[move-to-next-xref]  Skip to next hypertext link [*].\n"
msgstr ""
"\\%-10[move-to-next-xref]  Saltar al siguiente vínculo de hipertexto [*].\n"

#: info/infodoc.c:62
msgid "\\%-10[move-to-prev-xref]  Skip to previous hypertext link [*].\n"
msgstr ""
"\\%-10[move-to-prev-xref] Saltar al anterior vínculo de hipertexto [*].\n"

#: info/infodoc.c:63
msgid ""
"\\%-10[select-reference-this-line]  Follow the hypertext link under cursor.\n"
msgstr ""
"\\%-10[select-reference-this-line] Sigue el enlace de hipertexto debajo del "
"cursor.\n"

#: info/infodoc.c:64
msgid ""
"\\%-10[dir-node]  Move to the `directory' node.  Equivalent to `\\[goto-"
"node] (DIR)'.\n"
msgstr ""
"\\%-10[dir-node] Va al nodo `directory'. Equivalente a `\\[goto-node] "
"(DIR)'.\n"

#: info/infodoc.c:65
msgid ""
"\\%-10[top-node]  Move to the Top node.  Equivalent to `\\[goto-node] Top'.\n"
msgstr ""
"\\%-10[top-node] Va al nodo de mayor nivel. Equivalente a `\\[goto-node] "
"Top'.\n"

#: info/infodoc.c:70 info/infodoc.c:118
msgid ""
"Moving within a node:\n"
"---------------------\n"
msgstr ""
"Yendo a nodo:\n"
"---------------------\n"

#: info/infodoc.c:72
msgid "\\%-10[beginning-of-node]  Go to the beginning of this node.\n"
msgstr "\\%-10[beginning-of-node] Va al principio de este nodo.\n"

#: info/infodoc.c:73
msgid "\\%-10[end-of-node]  Go to the end of this node.\n"
msgstr "\\%-10[end-of-node] Va al final de este nodo.\n"

#: info/infodoc.c:74
msgid "\\%-10[next-line]  Scroll forward 1 line.\n"
msgstr "\\%-10[next-line] Desplaza una línea adelante.\n"

#: info/infodoc.c:75
msgid "\\%-10[prev-line]  Scroll backward 1 line.\n"
msgstr "\\%-10[prev-line] Desplaza una línea atrás.\n"

#: info/infodoc.c:76
msgid "\\%-10[scroll-forward]  Scroll forward a page.\n"
msgstr "\\%-10[scroll-forward] Desplaza una página adelante.\n"

#: info/infodoc.c:77
msgid "\\%-10[scroll-backward]  Scroll backward a page.\n"
msgstr "\\%-10[scroll-backward] Desplaza una página atrás.\n"

#: info/infodoc.c:79 info/infodoc.c:127
msgid ""
"Other commands:\n"
"---------------\n"
msgstr ""
"Otros comandos:\n"
"---------------\n"

#: info/infodoc.c:81
msgid "1...9       Pick first...ninth item in this node's menu.\n"
msgstr "1...9 escoja del primero al noveno elemento en este menú de nodo.\n"

#: info/infodoc.c:82
msgid "\\%-10[last-menu-item]  Pick last item in this node's menu.\n"
msgstr ""
"\\%-10[last-menu-item]  Seleccionar el último elemento en este menú de "
"nodo.\n"

#: info/infodoc.c:83
msgid ""
"\\%-10[index-search]  Search for a specified string in the index entries of "
"this Info\n"
"              file, and select the node referenced by the first entry "
"found.\n"
msgstr ""
"\\%-10[index-search] Busca una cadena específica en las entradas de índice "
"de\n"
"              archivo Info y selecciona el nodo referenciado por la primera "
"entrada encontrada.\n"

#: info/infodoc.c:85
msgid ""
"\\%-10[goto-node]  Move to node specified by name.\n"
"              You may include a filename as well, as in (FILENAME)NODENAME.\n"
msgstr ""
"\\%-10[goto-node] Va al nodo especificado por el nombre.\n"
"              Puede también incluir un nombre de archivo, como en "
"(FILENAME)NODENAME.\n"

#: info/infodoc.c:87
msgid ""
"\\%-10[search]  Search forward for a specified string\n"
"              and select the node in which the next occurrence is found.\n"
msgstr ""
"\\%-10[search] Busca hacia adelante una cadena específica\n"
"              y selecciona el nodo en el que se encuentra la próxima "
"ocurrencia.\n"

#: info/infodoc.c:89
msgid ""
"\\%-10[search-backward]  Search backward for a specified string\n"
"              and select the node in which the previous occurrence is "
"found.\n"
msgstr ""
"\\%-10[search-backward] Busca hacia atrás por una cadena específica\n"
"              y selecciona el nodo en el que se ha encontrado la anterior "
"ocurrencia.\n"

