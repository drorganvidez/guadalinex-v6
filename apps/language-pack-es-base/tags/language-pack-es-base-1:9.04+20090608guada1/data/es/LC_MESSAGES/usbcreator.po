#: .././usbcreator/gtk_frontend.py:231
msgid "CD-Drive/Image"
msgstr "Unidad de CD / Imagen"

#: .././usbcreator/gtk_frontend.py:240
msgid "OS Version"
msgstr "Versión del S.O."

#: .././usbcreator/gtk_frontend.py:246
msgid "Size"
msgstr "Tamaño"

#. TODO: i18n
#: .././usbcreator/gtk_frontend.py:261
msgid "ISO Files"
msgstr "Archivos ISO"

#: .././usbcreator/gtk_frontend.py:286
msgid "Please insert a CD or select 'Other...'."
msgstr "Inserte el CD o seleccione «Otro...»."

#: .././usbcreator/gtk_frontend.py:292
msgid "Please insert a USB stick."
msgstr "Inserte una memoria USB."

#: .././usbcreator/gtk_frontend.py:330 .././usbcreator/gtk_frontend.py:353
#, python-format
msgid "%s is too small for %s."
msgstr "%s es demasiado pequeño para %s."

#: .././usbcreator/gtk_frontend.py:333
#, python-format
msgid "%s needs to be formatted."
msgstr "%s necesita ser formateado."

#: .././usbcreator/gtk_frontend.py:340
#, python-format
msgid "%s has enough free space for %s."
msgstr "%s tiene espacio suficiente para %s."

#: .././usbcreator/gtk_frontend.py:361
#, python-format
msgid "%s is too full to fit %s (%s more needed)."
msgstr "%s está demasiado lleno para guardar %s (se necesitan %s más)."

#: .././usbcreator/gtk_frontend.py:413
msgid "Device"
msgstr "Dispositivo"

#: .././usbcreator/gtk_frontend.py:424
msgid "Label"
msgstr "Etiqueta"

#: .././usbcreator/gtk_frontend.py:430
msgid "Capacity"
msgstr "Capacidad"

#: .././usbcreator/gtk_frontend.py:436
msgid "Free Space"
msgstr "Espacio libre"

#: .././usbcreator/gtk_frontend.py:447
msgid "Starting up"
msgstr "Iniciando"

#: .././usbcreator/gtk_frontend.py:453
msgid "Install button pressed but there was no source selected."
msgstr ""
"Se pulsó el botón de instalar pero no había ningún origen seleccionado."

#: .././usbcreator/gtk_frontend.py:459
msgid "Install button pressed but there was no destination selected."
msgstr ""
"Se pulsó el botón de instalar pero no había ningún destino seleccionado."

#: .././usbcreator/gtk_frontend.py:476
#, python-format
msgid "%d%% complete"
msgstr "%d%% completado"

#: .././usbcreator/backend.py:128
msgid "Unable to create a partition table:"
msgstr "No se pudo crear una tabla de particiones:"

#: .././usbcreator/backend.py:134
msgid "Unable to format device:"
msgstr "No se pudo formatear el dispositivo:"

#: .././usbcreator/backend.py:411
#, python-format
msgid ""
"Unable to mount the image %s.\n"
"\n"
"Please see ~/.usb-creator.log for more details"
msgstr ""
"No se pudo montar la imagen %s.\n"
"\n"
"Consulte ~/.usb-creator.log para más detalles"

#: .././usbcreator/backend.py:431
msgid ""
"This is not a desktop install CD and thus cannot be used by this application."
msgstr ""
"Este no es un CD de instalación de escritorio y por lo tanto no puede usarse "
"en esta aplicación."

#. TODO: Needs to be moved into the generic install routine.
#. Ugly, i18n.
#: .././usbcreator/backend.py:444
msgid "Installing the bootloader"
msgstr "Instalando el cargador de arranque"

#: .././usbcreator/backend.py:460
msgid "Unable to determine the partition number."
msgstr "No se pudo determinar el número de partición."

#: .././usbcreator/backend.py:466
msgid "Error installing the bootloader."
msgstr "Error instalando el cargador de arranque."

#: .././gui/usbcreator.glade.h:1
msgid "Discarded on shutdown, unless you save them elsewhere"
msgstr "Se perderán al apagar a menos que los guarde en otro sitio"

#: .././gui/usbcreator.glade.h:2
msgid "Do you really want to quit the installation now?"
msgstr "¿Desea salir de la instalación?"

#: .././gui/usbcreator.glade.h:3
msgid "Format"
msgstr "Formatear"

#: .././gui/usbcreator.glade.h:4
msgid "How much:"
msgstr "Espacio:"

#: .././gui/usbcreator.glade.h:5
msgid "Installation Complete"
msgstr "Instalación completa"

#: .././gui/usbcreator.glade.h:6
msgid "Installation Failed"
msgstr "La instalación ha fallado"

#: .././gui/usbcreator.glade.h:7
msgid ""
"Installation is complete.  You may now reboot your computer with this USB "
"thumb drive inserted to boot Ubuntu."
msgstr ""
"Instalación completa. Para iniciar Ubuntu, reinicie el equipo con la unidad "
"USB insertada."

#: .././gui/usbcreator.glade.h:8
msgid "Make Startup Disk"
msgstr "Crear disco de inicio"

#: .././gui/usbcreator.glade.h:9
msgid "Make USB Startup Disk"
msgstr "Crear disco USB de inicio"

#: .././gui/usbcreator.glade.h:10
msgid "Other..."
msgstr "Otro..."

#: .././gui/usbcreator.glade.h:11
msgid "Quit the installation?"
msgstr "¿Salir de la instalación?"

#: .././gui/usbcreator.glade.h:12
msgid "Source disc image (.iso) or CD:"
msgstr "CD o imagen (.iso) de origen:"

#: .././gui/usbcreator.glade.h:13
msgid "Stored in reserved extra space"
msgstr "Se guardarán en un espacio reservado"

#: .././gui/usbcreator.glade.h:14
msgid ""
"The installation failed.  Please see ~/.usb-creator.log for more details.\n"
msgstr ""
"La instalación ha fallado. Consulte los detalles en ~/.usb-creator.log.\n"

#: .././gui/usbcreator.glade.h:16
msgid ""
"To try or install Ubuntu from a USB disk, it needs to be set up as a startup "
"disk."
msgstr ""
"Para probar o instalar Ubuntu desde un disco USB, necesitará configurarlo "
"como un disco de inicio."

#: .././gui/usbcreator.glade.h:17
msgid "USB disk to use:"
msgstr "Disco USB de destino:"

#: .././gui/usbcreator.glade.h:18
msgid "When starting up from this disk, documents and settings will be:"
msgstr "Al iniciar desde el disco, los documentos y la configuración:"

#: .././desktop/usb-creator.desktop.in.h:1
msgid "Create a USB startup disk using a CD or disc image"
msgstr "Crear un disco de inicio USB usando un CD o una imagen de disco"

#: .././desktop/usb-creator.desktop.in.h:2
msgid "USB Startup Disk Creator"
msgstr "Creador de discos USB de inicio"
