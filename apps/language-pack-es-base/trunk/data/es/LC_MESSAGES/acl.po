msgstr "%s: %s: No se puede cambiar el propietario/grupo: %s\n"

#: ../setfacl/setfacl.c:233
#, c-format
msgid "%s %s -- set file access control lists\n"
msgstr "%s %s -- establecer archivo de listas de control de acceso\n"

#: ../setfacl/setfacl.c:235 ../setfacl/setfacl.c:614
#, c-format
msgid "Usage: %s %s\n"
msgstr "Uso: %s %s\n"

#: ../setfacl/setfacl.c:238
#, c-format
msgid ""
"  -m, --modify=acl        modify the current ACL(s) of file(s)\n"
"  -M, --modify-file=file  read ACL entries to modify from file\n"
"  -x, --remove=acl        remove entries from the ACL(s) of file(s)\n"
"  -X, --remove-file=file  read ACL entries to remove from file\n"
"  -b, --remove-all        remove all extended ACL entries\n"
"  -k, --remove-default    remove the default ACL\n"
msgstr ""
"  -m, --modify=acl modifica la(s) ACL(s) actuales del archivo(s)\n"
"  -M, --modify-file=file lee del archivo las entradas de la ACL para "
"modificar \n"
"  -x, --remove=acl elimina entradas de las ACL(s) del archivo(s)\n"
"  -X, --remove-file=file lee entradas de la ACL para quitar del archivo\n"
"  -b, --remove-all elimina todas las entradas extendidas de la ACL\n"
"  -k, --remove-default elimina la ACL predeterminada\n"

#: ../setfacl/setfacl.c:247
#, c-format
msgid ""
"      --set=acl           set the ACL of file(s), replacing the current ACL\n"
"      --set-file=file     read ACL entries to set from file\n"
"      --mask              do recalculate the effective rights mask\n"
msgstr ""
"      --set=acl establece la ACL del archivo(s), sustituyendo la ACL actual\n"
"      --set-file=file lee del archivo las entradas de la ACL a establecer\n"
"      --mask recalcula la máscara efectiva de privilegios\n"

#: ../setfacl/setfacl.c:253
#, c-format
msgid ""
"  -n, --no-mask           don't recalculate the effective rights mask\n"
"  -d, --default           operations apply to the default ACL\n"
msgstr ""
"  -n, --no-mask no recalcula la máscara efectiva de privilegios\n"
"  -d, --default las operaciones se aplican sobre la ACL predeterminada\n"

#: ../setfacl/setfacl.c:258
#, c-format
msgid ""
"  -R, --recursive         recurse into subdirectories\n"
"  -L, --logical           logical walk, follow symbolic links\n"
"  -P, --physical          physical walk, do not follow symbolic links\n"
"      --restore=file      restore ACLs (inverse of `getfacl -R')\n"
"      --test              test mode (ACLs are not modified)\n"
msgstr ""
"  -R, --recursive operar recursivamente dentro de los subdirectorios\n"
"  -L, --logical seguir los enlaces simbólicos\n"
"  -P, --physical no seguir los enlaces simbólicos\n"
"      --restore=file restaurar las ACLs (operación inversa a `getfacl -R')\n"
"      --test modo de pruebas (las ACLs no se modifican)\n"

#: ../setfacl/setfacl.c:266 ../getfacl/getfacl.c:573
#, c-format
msgid ""
"      --version           print version and exit\n"
"      --help              this help text\n"
msgstr ""
"      --version muestra la versión y sale\n"
"      --help éste texto de ayuda\n"

#: ../setfacl/setfacl.c:280 ../getfacl/getfacl.c:714
#, c-format
msgid "%s: Standard input: %s\n"
msgstr "%s: Entrada estándar: %s\n"

#: ../setfacl/setfacl.c:416
#, c-format
msgid "%s: Option -%c incomplete\n"
msgstr "%s: Opción -%c incompleta\n"

#: ../setfacl/setfacl.c:421
#, c-format
msgid "%s: Option -%c: %s near character %d\n"
msgstr "%s: Opción -%c: %s cerca del carácter %d\n"

#: ../setfacl/setfacl.c:497
#, c-format
msgid "%s: %s in line %d of file %s\n"
msgstr "%s: %s en la línea %d del archivo %s\n"

#: ../setfacl/setfacl.c:505
#, c-format
msgid "%s: %s in line %d of standard input\n"
msgstr "%s: %s en la línea %d de la entrada estándar\n"

#: ../setfacl/setfacl.c:616 ../getfacl/getfacl.c:729
#, c-format
msgid "Try `%s --help' for more information.\n"
msgstr "Ejecute `%s --help' para más información.\n"

#: ../getfacl/getfacl.c:477
#, c-format
msgid "%s: Removing leading '/' from absolute path names\n"
msgstr "%s: Quitar el prefijo '/' de las rutas absolutas\n"

#: ../getfacl/getfacl.c:546
#, c-format
msgid "%s %s -- get file access control lists\n"
msgstr "%s %s -- recibir listas de control de acceso de archivo\n"

#: ../getfacl/getfacl.c:548 ../getfacl/getfacl.c:727
#, c-format
msgid "Usage: %s [-%s] file ...\n"
msgstr "Uso: %s [-%s] archivo ...\n"

#: ../getfacl/getfacl.c:554
#, c-format
msgid "  -d, --default           display the default access control list\n"
msgstr ""
"  -d, --default mostrar la lista de control de acceso predeterminada\n"

#: ../getfacl/getfacl.c:558
#, c-format
msgid ""
"      --access            display the file access control list only\n"
"  -d, --default           display the default access control list only\n"
"      --omit-header       do not display the comment header\n"
"      --all-effective     print all effective rights\n"
"      --no-effective      print no effective rights\n"
"      --skip-base         skip files that only have the base entries\n"
"  -R, --recursive         recurse into subdirectories\n"
"  -L, --logical           logical walk, follow symbolic links\n"
"  -P, --physical          physical walk, do not follow symbolic links\n"
"      --tabular           use tabular output format\n"
"      --numeric           print numeric user/group identifiers\n"
"      --absolute-names    don't strip leading '/' in pathnames\n"
msgstr ""
"      --access            muestra sólo la lista de control de acceso del "
"archivo\n"
"  -d, --default           muestra sólo la lista de control de acceso "
"predeterminada\n"
"      --omit-header       no muestra la cabecera del comentario\n"
"      --all-effective     imprime todos los derechos efectivos\n"
"      --no-effective      imprime los derechos no efectivos\n"
"      --skip-base         omite archivos que tienen sólo entradas base\n"
"  -R, --recursive         recurre en los subdirectorios\n"
"  -L, --logical           camino lógico, sigue los vínculos simbólicos\n"
"  -P, --physical          camino físico, no sigue los vínculos simbólicos\n"
"      --tabular           usa formato de salida tabular\n"
"      --numeric           imprime identificadores numéricos de "
"usuario/grupo\n"
"      --absolute-names    no quita «/» en los nombres de ruta\n"

