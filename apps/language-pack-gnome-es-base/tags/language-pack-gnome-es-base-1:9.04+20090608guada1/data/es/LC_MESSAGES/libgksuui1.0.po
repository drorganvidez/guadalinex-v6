#: libgksuui/gksuui-dialog.c:102
msgid "Continue"
msgstr "Continuar"

#: libgksuui/gksuui-dialog.c:130
msgid ""
"<span weight=\"bold\" size=\"larger\">Type the root password.</span>\n"
msgstr ""
"<span weight=\"bold\" size=\"larger\">Escriba la contraseña de root.</span>\n"

#: libgksuui/gksuui-dialog.c:148
msgid "Password:"
msgstr "Contraseña:"

#: libgksuui/gksuui-convenience.c:59
#, c-format
msgid ""
"libgksu: Failed to init the Gtk+ library, GKSu is not able to continue.\n"
msgstr ""
"libgksu: Falló al inicializar la biblioteca Gtk+, GKSu no puede continuar.\n"

#: libgksuui/gksuui-convenience.c:145
msgid ""
"Could not grab your mouse.\n"
"A malicious client may be eavesdropping\n"
"on your session."
msgstr ""
"No se pudo capturar su ratón.\n"
"Quizá haya algún cliente malicioso espiando\n"
"su sesión."

#: libgksuui/gksuui-convenience.c:151
msgid ""
"Could not grab your keyboard.\n"
"A malicious client may be eavesdropping\n"
"on your session."
msgstr ""
"No se puede bloquear su teclado.\n"
"Quizá haya algún cliente malicioso espiando\n"
"su sesión."
