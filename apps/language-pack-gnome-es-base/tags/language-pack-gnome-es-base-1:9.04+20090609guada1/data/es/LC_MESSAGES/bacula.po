#: src/stored/bscan.c:115
#, c-format
msgid ""
"\n"
"Version: %s (%s)\n"
"\n"
"Usage: bscan [ options ] <bacula-archive>\n"
"       -b bootstrap      specify a bootstrap file\n"
"       -c <file>         specify configuration file\n"
"       -d <nn>           set debug level to <nn>\n"
"       -m                update media info in database\n"
"       -n <name>         specify the database name (default bacula)\n"
"       -u <user>         specify database user name (default bacula)\n"
"       -P <password>     specify database password (default none)\n"
"       -h <host>         specify database host (default NULL)\n"
"       -p                proceed inspite of I/O errors\n"
"       -r                list records\n"
"       -s                synchronize or store in database\n"
"       -S                show scan progress periodically\n"
"       -v                verbose\n"
"       -V <Volumes>      specify Volume names (separated by |)\n"
"       -w <dir>          specify working directory (default from conf file)\n"
"       -?                print this message\n"
"\n"
msgstr ""
"\n"
"Versión: %s (%s)\n"
"\n"
"Uso: bscan [ opciones ] <archivo-bacula>\n"
"       -b bootstrap      especifica un archivo de arranque\n"
"       -c <file>         especifica un archivo de configuración\n"
"       -d <nn>           establece el nivel de depuración a <nn>\n"
"       -m                actualiza la información de la base de datos de "
"medios\n"
"       -n <name>         especifica el nombre de la base de datos "
"(predeterminada bacula)\n"
"       -u <user>         especifica el nombre del usuario de la base de "
"datos (predeterminado bacula)\n"
"       -P <password>     especifica la contraseña de la base de datos "
"(predeterminada ninguna)\n"
"       -h <host>         especifica el anfitrión de la base de datos "
"(predeterminado NULL)\n"
"       -p                proceder a pesar de errores de E/S\n"
"       -r                lista de registros\n"
"       -s                sincroniza el almacén en la base de datos\n"
"       -S                muestra el progreso de la exploración "
"periódicamente\n"
"       -v                verboso\n"
"       -V <Volumes>      especifica los nombre de los volúmenes (separados "
"por |)\n"
"       -w <dir>          especifica el directorio de trabajo (predeterminado "
"para el archivo conf)\n"
"       -?                muestra este mensaje\n"
"\n"

