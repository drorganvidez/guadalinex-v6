#: config-desktop/C/config-desktop.xml:60(para) 
msgid "This topic provides instructions on how to install and use a desktop other than the default GNOME desktop, such as KDE or Xfce."
msgstr ""

#: config-desktop/C/config-desktop.xml:63(title) 
msgid "Installing a KDE desktop"
msgstr ""

#: config-desktop/C/config-desktop.xml:64(para) 
msgid "<ulink url=\"http://www.kde.org/\">KDE</ulink> is a popular, fully-featured desktop environment. <ulink url=\"http://www.kubuntu.org\">Kubuntu</ulink> is a version of Ubuntu which uses the KDE desktop."
msgstr ""

#: config-desktop/C/config-desktop.xml:65(para) 
msgid "To install and use KDE:"
msgstr ""

#: config-desktop/C/config-desktop.xml:68(para) 
msgid "Use the <ulink type=\"help\" url=\"ghelp:add-applications#synaptic\">Synaptic Package Manager</ulink> to install the <application>kubuntu-desktop</application> package."
msgstr ""

#: config-desktop/C/config-desktop.xml:71(para) config-desktop/C/config-desktop.xml:94(para) 
msgid "Press <menuchoice><guimenu>System</guimenu><guimenuitem>Quit...</guimenuitem></menuchoice> and then press <guibutton>Log Out</guibutton> to log out of Ubuntu"
msgstr ""

#: config-desktop/C/config-desktop.xml:74(para) config-desktop/C/config-desktop.xml:97(para) 
msgid "At the log-in screen which appears, press <menuchoice><guimenuitem>Options</guimenuitem><guimenuitem>Select Session...</guimenuitem></menuchoice>"
msgstr ""

#: config-desktop/C/config-desktop.xml:77(para) 
msgid "Choose the <guilabel>KDE</guilabel> option and press <guibutton>Change Session</guibutton>"
msgstr ""

#: config-desktop/C/config-desktop.xml:80(para) 
msgid "Enter your username and password as normal. KDE should then start."
msgstr ""

#: config-desktop/C/config-desktop.xml:86(title) 
msgid "Installing an Xfce desktop"
msgstr ""

#: config-desktop/C/config-desktop.xml:87(para) 
msgid "<ulink url=\"http://www.xfce.org/\">Xfce</ulink> is a desktop environment which is designed to be fast and lightweight. <ulink url=\"http://www.xubuntu.org\">Xubuntu</ulink> is a version of Ubuntu which uses the Xfce desktop."
msgstr ""

#: config-desktop/C/config-desktop.xml:88(para) 
msgid "To install and use Xfce:"
msgstr ""

#: config-desktop/C/config-desktop.xml:91(para) 
msgid "Use the <ulink type=\"help\" url=\"ghelp:add-applications#synaptic\">Synaptic Package Manager</ulink> to install the <application>xubuntu-desktop</application> package."
msgstr ""

#: config-desktop/C/config-desktop.xml:100(para) 
msgid "Choose the <guilabel>Xfce</guilabel> option and press <guibutton>Change Session</guibutton>"
msgstr ""

#: config-desktop/C/config-desktop.xml:103(para) 
msgid "Enter your username and password as normal. Xfce should then start."
msgstr ""

