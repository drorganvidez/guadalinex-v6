msgid ""
"<ulink url=\"apt:anjuta\">Install the <application>anjuta</application> "
"package</ulink>."
msgstr ""
"<ulink url=\"apt:anjuta\">Install the <application>anjuta</application> "
"package</ulink>."

#: programming/C/programming.xml:81(para)
